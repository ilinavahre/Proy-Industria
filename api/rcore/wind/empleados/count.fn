
(sentinel::privilege-required "Administrador")

(call "@filters")

(db::row `
    SELECT
        COUNT(DISTINCT x.user_id) `count`

    FROM ##users x
    INNER JOIN ##user_privileges up ON up.user_id=x.user_id
    INNER JOIN ##privileges p ON p.privilege_id=up.privilege_id

    WHERE {filters}
`)
